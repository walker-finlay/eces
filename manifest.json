{
  "name": "Epicurious Enhancement Suite",
  "description": "Adds various functionality to epicurious",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://www.epicurious.com/recipes/food/views/*"
      ],
      "run_at": "document_start",
      "css": [
        "notebox.css"
      ],
      "js": [
        "notebox.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "/images/eces16.png",
    "32": "/images/eces32.png",
    "48": "/images/eces48.png",
    "128": "/images/eces128.png"
  }
}